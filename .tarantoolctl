default_cfg = {
    pid_file   = "/usr/local/var/run/tarantool", -- /usr/local/var/run/tarantool/${INSTANCE}.pid
    wal_dir    = "/usr/local/var/lib/tarantool", -- /usr/local/var/lib/tarantool/${INSTANCE}/
    memtx_dir  = "/usr/local/var/lib/tarantool", -- /usr/local/var/lib/tarantool/${INSTANCE}
    vinyl_dir  = "/usr/local/var/lib/tarantool", -- /usr/local/var/lib/tarantool/${INSTANCE}
    log        = "/usr/local/var/log/tarantool", -- /usr/local/var/log/tarantool/${INSTANCE}.log
    username   = "root",
}

-- instances.available - all available instances
-- instances.enabled - instances to autostart by sysvinit
instance_dir = "/usr/local/etc/tarantool/instances.available"
